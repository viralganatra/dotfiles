  Protocol 2
  ServerAliveInterval 60
  Compression yes
  TCPKeepAlive yes

Host github.com
  AddKeysToAgent yes
  ForwardAgent yes
  HostName github.com
  IdentityFile ~/.ssh/foo
  UseKeychain yes
  User git

